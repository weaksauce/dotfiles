-h ".php.inc"
--exclude="\.git"
--totals=yes
--tag-relative=yes
--PHP-kinds=cidf
--regex-PHP=/abstract\s+class\s+([^ ]+)/\1/c,class/
--regex-PHP=/interface\s+([^ ]+)/\1/i,interface/
--regex-PHP=/(public\s+|static\s+|abstract\s+|protected\s+|private\s+)function\s+\&?\s*([^ (]+)/\2/f,function/
--langdef=planr
--langmap=planr:.planr
--regex-planr=/^\[(.*)\]$/|--\1/h,heading/
--regex-planr=/^(\t| {4}){1}\[(.*)\]$/|    |--\2/h,subheading/
--regex-planr=/^(\t| {4}){2}\[(.*)\]$/|         |--\2/h,subsubheading/
--langdef=css
--langmap=css:.css
--regex-css=/^[ \t]*([^\t {][^{]{1,100})(\t| )*{/\1/f,definition/
--langdef=scss
--langmap=scss:.scss
--regex-scss=/^[ \t]*([^\t {][^{]{1,100})(\t| )*{/| \1/f,definition/
